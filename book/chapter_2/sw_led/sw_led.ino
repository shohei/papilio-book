#define SW 31 
#define LED 40 
void setup() {                
  pinMode(SW, INPUT);  
  pinMode(LED, OUTPUT);   
}
void loop() {
  unsigned int sw_n;
  sw_n = ! digitalRead(SW);  // read SW
  digitalWrite(LED, sw_n);   // set LED
}
